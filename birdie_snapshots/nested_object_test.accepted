import gleam/option.{type Option, None}
import oas/generator/utils
import gleam/json
import gleam/dynamic
import gleam/dynamic/decode
import gleam/dict

pub type BigBird {
  BigBird(little_bird: Anon46a41d9d)
}

pub type BigBox {
  BigBox(little_box: Anon7abc434f)
}

pub type Anon46a41d9d {
  Anon46a41d9d(fish: Int)
}

pub type Anon7abc434f {
  Anon7abc434f(fish: String)
}

pub fn anon_7abc434f_decoder() {
  use fish <- decode.field("fish", decode.string)
  decode.success(Anon7abc434f(fish: fish))
}

pub fn anon_7abc434f_encode(data: Anon7abc434f) {
  utils.object([#("fish", json.string(data.fish))])
}

pub fn anon_46a41d9d_decoder() {
  use fish <- decode.field("fish", decode.int)
  decode.success(Anon46a41d9d(fish: fish))
}

pub fn anon_46a41d9d_encode(data: Anon46a41d9d) {
  utils.object([#("fish", json.int(data.fish))])
}

pub fn big_box_decoder() {
  use little_box <- decode.field("little_box", anon_7abc434f_decoder())
  decode.success(BigBox(little_box: little_box))
}

pub fn big_box_encode(data: BigBox) {
  utils.object([#("little_box", anon_7abc434f_encode(data.little_box))])
}

pub fn big_bird_decoder() {
  use little_bird <- decode.field("little_bird", anon_46a41d9d_decoder())
  decode.success(BigBird(little_bird: little_bird))
}

pub fn big_bird_encode(data: BigBird) {
  utils.object([#("little_bird", anon_46a41d9d_encode(data.little_bird))])
}
