import gleam/option.{type Option, None}
import gleam/json
import gleam/dynamic/decode

pub type User {
  User(name: String)
}

pub fn user_decoder() {
  use name <- decode.field("name", decode.string)
  decode.success(User(name: name))
}

pub fn user_encode(data: User) {
  json.object([#("name", json.string(data.name))])
}
